<h1 class="text-center">Tripulante <%= @member.id %> </h1>
<p><%= @member.name %></p>
<p><%= @member.CPF %></p>
<p><%= @member.birth %></p>

<%=link_to "Editar", edit_member_path(@member), class: "btn btn-success m-4"%>
<%= link_to "Deletar", member_path(@member), method: :delete, data: { confirm: "Está certo disso?" }, class: "btn btn-success m-4" %>
<%=link_to "Voltar Lista de Tripulantes", members_path, class: "btn btn-success m-4"%>
<%=link_to "Criar Associação", new_crew_path, class: "btn btn-success m-4"%>
<%=link_to "Lista de Tripulações", crews_path, class: "btn btn-success m-4"%>
